# Ifs
snippet if
	if $1:
		${0:${VISUAL}}
endsnippet
snippet el
	else:
		${0:${VISUAL}}
endsnippet
snippet ei
	elif $1:
		${0:${VISUAL}}
endsnippet

# for loop
snippet for
	for ${1:item} in ${2:items}:
		${0}
endsnippet

# while loop
snippet wh
	while $1:
		${0:${VISUAL}}
endsnippet

# print
snippet pr
	print($0)
endsnippet

		
